													<Column
														hAlign="Begin"
														popinDisplay="Inline"
														demandPopin="true"
														minScreenWidth="Tablet">
														<Label text="Código"/>
													</Column>
													<Column
														hAlign="Begin"
														popinDisplay="Inline"
														demandPopin="true"
														minScreenWidth="Tablet">
														<Label text="Planta"/>
													</Column>
													<Column
														hAlign="Begin"
														popinDisplay="Inline"
														demandPopin="true"
														minScreenWidth="Tablet">
														<Label text="Descripción"/>
													</Column>
													<Column
														hAlign="End"
														popinDisplay="Inline"
														demandPopin="true"
														minScreenWidth="Tablet">
														<Label text="Almacén Externo"/>
