                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true" id="idFormCocinero">
                                            <form:label>
                                                <Label text="Cocinero" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                            
                                             <Input   class="sapUiSmallMarginBottom"
												id="idCocinero"
												width="100%"
                                                description=""
                                                change="traeCocinero"
												placeholder="Ingresar Cocinero..."
												showSuggestion="true"
												showValueHelp="false"
												valueState="Information"
												valueStateText="Ingrese un texto"
												suggestionRows="{Cocinero>/listaCocinero}">
												<suggestionColumns>
													
													<Column
														hAlign="Begin"
														popinDisplay="Inline"
														demandPopin="true"
														minScreenWidth="Tablet">
														<Label text="Código"/>
													</Column>
													<Column
														hAlign="Begin"
														popinDisplay="Inline"
														demandPopin="true"
														minScreenWidth="Tablet">
