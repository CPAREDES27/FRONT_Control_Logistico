<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:f="sap.f"
	xmlns:fb="sap.ui.comp.filterbar" xmlns:vm="sap.ui.comp.variants" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout"
	xmlns:t="sap.ui.table" xmlns:dnd="sap.ui.core.dnd" height="100%" xmlns:form="sap.ui.layout.form"
	xmlns:filterbar="sap.ui.comp.filterbar" xmlns:l="sap.ui.layout" xmlns:variants="sap.ui.comp.variants">
    <Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Vale de víveres" type="Standard" verticalScrolling="true">
        <content>
            <form:Form width="100%" editable="true" visible="true">
                    <form:title/>
                            <form:formContainers>
                                <form:FormContainer visible="true">
                                    <form:formElements>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Vale" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/NRVVI}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Temporada" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/DSTPO}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Planta" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <HBox>
                                                    <Text text="{modelValeViveres>/VerDetalle/CDPTA}" visible="true" width="auto"/>
                                                    <Text text=" " width="20px" textAlign="Center" />
                                                    <Text text="{modelValeViveres>/VerDetalle/DESCR}" visible="true" width="auto"/>
                                                </HBox>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Alamacén" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <HBox>
                                                    <Text text="{modelValeViveres>/VerDetalle/CDALM}" visible="true" width="auto"/>
                                                    <Text text=" " width="20px" textAlign="Center" />
                                                    <Text text="{modelValeViveres>/VerDetalle/DSALM}" visible="true" width="auto"/>
                                                </HBox>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Embarcación" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <HBox>
                                                    <Text text="{modelValeViveres>/VerDetalle/CDEMB}" visible="true" width="auto"/>
                                                    <Text text=" " width="20px" textAlign="Center" />
                                                    <Text text="{modelValeViveres>/VerDetalle/NMEMB}" visible="true" width="auto"/>
                                                </HBox>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Matricula" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/MREMB}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Armador comerc" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/ARCMC}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Inicio travesía" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/FITVS}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Fin de travesía" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/FFTVS}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Nro. Tripulantes" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/NRTRI}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Cocinero" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <HBox>
                                                    <Text text="{modelValeViveres>/VerDetalle/PERNR}" visible="true" width="auto"/>
                                                    <Text text=" " width="20px" textAlign="Center" />
                                                    <Text text="{modelValeViveres>/VerDetalle/NMPER}" visible="true" width="auto"/>
                                                </HBox>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Proveedor" design="Standard" width="100%"  required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/CDPVE}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Nro. orden de compra" design="Standard" width="100%"  required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/EBELN}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Observaciones" design="Standard" width="100%" required="false"  textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/OBVVI}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Importe de víveres incluido IGV S/." design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/QTSUM}"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                    <form:title/>
                                </form:FormContainer>
                                <form:FormContainer visible="true">
                                    <form:formElements>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Estado de registro" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/ESVVI}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Fecha de creación" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/FCVVI}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Centro" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/WERKS}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Almacén externo" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/LGORT}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Indicador de Propiedad" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/DESC_INPRP}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="RUC armador comerc" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/STCD1}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Duración de travesía" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/DRTVS}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="RUC proveedor" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/RUCCP}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement visible="true">
                                            <form:label>
                                                <Label text="Nro. documento" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                            </form:label>
                                            <form:fields>
                                                <Text text="{modelValeViveres>/VerDetalle/BELNR}" visible="true" width="auto"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                    <form:title/>
                                </form:FormContainer>
                            </form:formContainers>
                            <form:layout>
                                <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                            </form:layout>
                        </form:Form>

                    <t:Table id="" rows="{ path:'modelValeViveres>/ListaSuministro', templateShareable:true}" width="auto" selectionMode="None" class="sapUiResponsiveMargin" visibleRowCount="{modelValeViveres>/RowListaSuministro}">
                        <t:columns>
                            <t:Column width="auto">
                                <Label text="Fecha"/>
                                <t:template>
                                    <Text text="{modelValeViveres>FECHA}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                            <t:Column width="auto">
                                <Label text="Posición"/>
                                <t:template>
                                    <Text text="{modelValeViveres>NRPOS}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                            <t:Column width="auto">
                                <Label text="Suministro"/>
                                <t:template>
                                    <Text text="{modelValeViveres>DSSUM}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                            <t:Column width="auto">
                                <Label text="Ración"/>
                                <t:template>
                                    <Text text="{modelValeViveres>CNRAC}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                            <t:Column width="auto">
                                <Label text="Unidad de medida"/>
                                <t:template>
                                    <Text text="{modelValeViveres>DSUMD}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                            <t:Column width="auto">
                                <Label text="Costo unitario"/>
                                <t:template>
                                    <Text text="{modelValeViveres>CUSUM}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                            <t:Column width="auto">
                                <Label text="Costo total"/>
                                <t:template>
                                    <Text text="{modelValeViveres>QTSUM}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                            <t:Column width="auto">
                                <Label text="Observaciónes"/>
                                <t:template>
                                    <Text text="{modelValeViveres>OBPVA}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </t:template>
                            </t:Column>
                        </t:columns>
                    </t:Table>

                    </content>
                    <beginButton/>
                            <buttons>
                                <Button text="Imprimir" type="Emphasized" icon="sap-icon://pdf-attachment" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onImprimirDetalle"/>
                                <Button text="Cancelar" type="Reject" icon="sap-icon://begin" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onCloseDialogVerDetalle"/>
                            </buttons>
                            <customHeader/>
                        <endButton/>
                    <subHeader/>
				</Dialog>
</core:FragmentDefinition>
