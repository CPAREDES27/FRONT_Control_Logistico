    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "tasa.com.valedeviveres.view.Main",
            "type": "XML",
            "async": true,
            "id": "Main"
        },
        "dependencies": {
            "minUI5Version": "1.95.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
		"config": {
			"sample": {
				"files": [
                    "DlgAlmacen.fragment.xml",
                    "DlgArmadorComercial.fragment.xml",
                    "DlgEmbarcacion.fragment.xml",
                    "DlgNewReg.fragment.xml",
                    "DlgPlanta.fragment.xml",
                    "DlgCocinero.fragment.xml",
                    "RegistrarValeViveres.view.xml",
                    "RegistrarValeViveres.controller.js",
                    "Main.view.xml",
                    "Main.controller.js",
					"manifest.json"
				]
			}
		},
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "tasa.com.valedeviveres.i18n.i18n"
                }
            },
            "modelValeViveres": {
                "preload": true,
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "tasa.com.valedeviveres.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": true
            },
            "routes": [
				{
					"pattern": "Main/:context:",
					"name": "Main",
					"target": [
						"Main"
					]
				},
				{
					"pattern": "",
					"name": "default",
					"target": [
						"Main"
					]
				},
				{
					"pattern": "RegistrarValeViveres/:context:",
					"name": "RegistrarValeViveres",
					"target": [
						"RegistrarValeViveres"
					]
				}
            ],
            "targets": {
				"Main": {
					"controlAggregation": "pages",
					"viewName": "Main",
					"viewLevel": 1,
                    "transition": "slide",
                    "clearControlAggregation": true
				},
				"RegistrarValeViveres": {
					"controlAggregation": "pages",
					"viewName": "RegistrarValeViveres",
					"viewLevel": 2,
                    "transition": "slide",
                    "clearControlAggregation": true
				}                
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "ControlLogisticoQAS"
    }
}